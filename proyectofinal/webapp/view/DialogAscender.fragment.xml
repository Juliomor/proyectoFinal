<core:FragmentDefinition 
    xmlns:core = "sap.ui.core"
    xmlns = "sap.m">
    <Dialog id = "_IDGenDialog1" title = "{i18n>nuevoAscenso}" titleAlignment = "Center">
        <content>
            <VBox id = "_IDGenVBox1" class = "sapUiSmallMargin">
                <Text id = "_IDGenText1" text = "Salario:" />
                <Input id = "_IDGenInput1" class = "sapUiSmallMarginBottom"/>
                <Text id = "_IDGenText2" text = "Fecha: "/>
                <DatePicker id = "_IDGenDatePicker1" class = "sapUiSmallMarginBottom"/>
                <Text id = "_IDGenText3" text = "Comentario: " />
                <TextArea id = "_IDGenTextArea1" width = "100%"/>
            </VBox>
        </content>
        <buttons>
            <Button id = "_IDGenButton1" text = "{i18n>aceptar}" press = "onAceptarAscenso"/>
            <Button id = "_IDGenButton2" text = "{i18n>cancelar}" press = "onCancelarAscenso"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>